{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel -d ./dist ./src -s",
    "build": "yarn run clean && yarn run build-babel ",
    "prod": "NODE_ENV=production  babel-node ./dist/App.js ",
    "start": "yarn build && yarn prod",
    "dev": "nodemon --harmony ./src/App.js --exec babel-node",
    "doc": "apidoc -i ./src/Docs -o ./public/docs && open ./public/docs/index.html",
    "heroku": "git push heroku master"
  },
  "engines": {
    "node": ">= 10.x"
  },
  "dependencies": {
    "aws-sdk": "^2.480.0",
    "bluebird": "^3.5.5",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0"
  },
  "heroku-run-build-script": true
}
